{
  "publisher": "tovi",
  "name": "apiary",
  "displayName": "Apiary",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "description": "",
  "author": "Tovi <tovi-cn@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/tovi-cn",
  "homepage": "https://github.com/tovi-cn/apiary#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tovi-cn/apiary"
  },
  "bugs": {
    "url": "https://github.com/tovi-cn/apiary/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/tovi-cn"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icon.png",
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onView:vsc-postman-side"
  ],
  "contributes": {
    "configuration": {
      "title": "VscPostman",
      "properties": {
        "vscPostman.history": {
          "type": "array",
          "default": []
        }
      }
    },
    "commands": [
      {
        "command": "vscPostman.new",
        "title": "Add",
        "icon": "$(add)"
      },
      {
        "command": "vscPostmanHistory.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "vscPostmanHistory.upload",
        "title": "Upload",
        "icon": "$(upload)"
      },
      {
        "command": "vscPostmanHistory.newWindow",
        "title": "Open in New Window",
        "icon": "$(link-external)"
      },
      {
        "command": "vscPostmanHistory.delete",
        "title": "Delete",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscPostman.new",
          "when": "view == vsc-postman-side",
          "group": "navigation"
        },
        {
          "command": "vscPostmanHistory.refresh",
          "when": "view == vsc-postman-side",
          "group": "navigation"
        },
        {
          "command": "vscPostmanHistory.upload",
          "when": "view == vsc-postman-side"
        }
      ],
      "view/item/context": [
        {
          "command": "vscPostmanHistory.newWindow",
          "when": "view == vsc-postman-side",
          "group": "inline"
        },
        {
          "command": "vscPostmanHistory.delete",
          "when": "view == vsc-postman-side",
          "group": "inline"
        }
      ]
    },
    "views": {
      "vsc-postman": [
        {
          "id": "vsc-postman-side",
          "name": "History"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "assets/rocket.svg",
          "id": "vsc-postman",
          "title": "Postman"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "vsc-postman-side",
        "contents": "%view.workbench.postman.disabled%"
      }
    ]
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "bumpp": "^8.2.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.3.2",
    "typescript": "^4.1.3",
    "vsce": "^2.14.0",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.21",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "axios": "^1.1.3"
  }
}
